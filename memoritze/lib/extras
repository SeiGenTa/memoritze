//!Input de consulta 1
        TextFormField(
          validator: (value) {
            if (value!.isEmpty) {
              return 'Debes ingresar consulta :c';
            }
            return null;
          },
          controller: keyQuestion,
          style: TextStyle(
            fontSize: 15,
            color: setting.getColorText(),
          ),
          decoration: InputDecoration(
            focusedBorder: OutlineInputBorder(
              borderSide: BorderSide(
                color: setting.getColorText(),
              ),
            ),
            enabledBorder: UnderlineInputBorder(
              borderSide: BorderSide(
                color: setting.getColorText(),
              ),
            ),
            labelStyle: TextStyle(color: setting.getColorText()),
            hintStyle: TextStyle(color: setting.getColorText()),
            hintText:
                "Recuerda poner una consulta lo suficientemente entendible y descriptiva",
            hoverColor: setting.getColorText(),
            labelText: "Pregunta",
            icon: Icon(
              Icons.near_me,
              color: setting.getColorText(),
            ),
          ),
        ),
        TextFormField(
          validator: (value) {
            if (value!.isEmpty) {
              return 'Â¿Como sabras si acertaste o no?';
            }
            return null;
          },
          controller: keyAnswer,
          style: TextStyle(
            fontSize: 15,
            color: setting.getColorText(),
